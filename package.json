{
  "name": "gatsby-custom-project-starter",
  "private": true,
  "description": "Default Gatsby Starter customized for the usage of TypeScript, TDD approach and linting experience which encourages to follow the best coding practices.",
  "version": "0.1.0",
  "author": "Tomasz Grande <grande.tomasz@gmail.com>",
  "keywords": [
    "gatsby",
    "jest",
    "react",
    "styled-components",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/grande-tomasz/gatsby-custom-project-starter"
  },
  "bugs": {
    "url": "https://github.com/grande-tomasz/gatsby-custom-project-starter/issues"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-google-tagmanager": "^2.3.3",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-styled-components": "^3.3.2",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "@types/testing-library__jest-dom": "^5.9.1",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.7",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "build": "npm run gatsby:clean && npm run gatsby:build && npm run gatsby:serve",
    "clean": "npm run gatsby:clean && rimraf node_modules && echo 'node modules deleted'",
    "eslint:check": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint:check -- --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "gatsby:build": "gatsby build",
    "gatsby:clean": "gatsby clean",
    "gatsby:develop": "gatsby develop",
    "gatsby:serve": "gatsby serve",
    "pre-commit": "npm run format && npm run eslint:fix && npm test",
    "pre-push": "npm run eslint:check && npm run stylelint:check && npm test",
    "start": "npm run gatsby:develop",
    "stylelint:check": "stylelint './src/**/*.tsx'",
    "test": "jest",
    "ts:check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}
